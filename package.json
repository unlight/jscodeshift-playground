{
  "name": "jscodeshift-playground",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "2020",
  "keywords": [],
  "bin": "./bin/cli.js",
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:r": "node -r ts-node/register/transpile-only node_modules/mocha/bin/_mocha src/**/*.spec.ts",
    "test:w": "npm run test:r -- --watch --watch-files src/**/*.ts",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix && npm run lint:doc:fix",
    "format:src": "prettier src --write",
    "build": "sh Taskfile build",
    "mocha": "node node_modules/mocha/bin/_mocha",
    "test:d": "ndb -r ts-node/register/transpile-only node_modules/mocha/bin/_mocha --no-timeouts --watch-files src/**/*.ts --watch src/**/*.spec.ts"
  },
  "dependencies": {
    "jscodeshift": "^0.13.1"
  },
  "devDependencies": {
    "@types/jscodeshift": "^0.11.3",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-total-functions": "^5.0.1",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^27.4.6",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "recast": "^0.20.5",
    "request": "^2.88.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "watchexec-bin": "^1.0.0"
  }
}
